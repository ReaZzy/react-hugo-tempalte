{
  "name": "react-hugo",
  "version": "1.0.0",
  "description": "React-Hugo with tailwind and typescript support",
  "scripts": {
    "start": "hugo -D --destination ./public && serve public",
    "start:dev": "cross-env NODE-ENV=development concurrently \"webpack --mode=development --watch\" \"hugo serve -D --disableFastRender\"",
    "start:debug": "webpack --mode=development --watch && hugo serve -D --debug",
    "build": "cross-env NODE-ENV=production webpack --mode=production && hugo"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "core-js": "^3.20.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-jsx": "^0.36.4",
    "postcss-loader": "^6.2.1",
    "prettier": "2.5.1",
    "prettier-eslint": "^13.0.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.18",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "4.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@tailwindcss/forms": "^0.4.0",
    "concurrently": "^7.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "tailwind": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
